{
  "name": "omnipedia-drupal",
  "version": "3.0.0",
  "license": "GPL-2.0",
  "repository": "gitlab:neurocracy/omnipedia/drupal",
  "packageManager": "yarn@3.2.4",
  "scripts": {
    "build:digitalOcean": "composer install --no-dev && yarn install && yarn build"
  },
  "workspaces": [
    "drupal/modules/ambientimpact",
    "drupal/modules/custom/*",
    "drupal/themes/ambientimpact",
    "drupal/themes/custom/*"
  ],
  "dependencies": {
    "ambientimpact-drupal-modules": "workspace:^5",
    "ambientimpact-drupal-themes": "workspace:^4",
    "drupal-omnipedia-block": "workspace:^3",
    "drupal-omnipedia-changes": "workspace:^3",
    "drupal-omnipedia-content": "workspace:^3",
    "drupal-omnipedia-media": "workspace:^3",
    "drupal-omnipedia-site-theme": "workspace:^3"
  }
}
