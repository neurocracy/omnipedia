  "license": "GPL-2.0-or-later",
  "repository": "github:neurocracy/omnipedia",
  "packageManager": "yarn@4.1.0",
  "engines": {
    "node": ">=20",
    "yarn": ">=4.1.0"
  },
  "scripts": {
    "build:production": "cross-env NODE_ENV=production yarn build",
