    "symfony/var-dumper": "^6.2"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "oomphinc/composer-installers-extender": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "php-http/discovery": true,
      "tbachert/spi": true
    },
    "preferred-install": {
      "drupal/ambientimpact_*": "source",
      "drupal/omnipedia_*": "source",
      "drupal/rebuilder": "source",
      "drupal/refreshless": "source"
    }
  },
  "autoload": {
    "psr-4": {
      "Omnipedia\\" : "src/"
    }
  },
  "extra": {
    "patches-file": "composer.patches.json",
    "patches-ignore": {
      "drupal/refreshless": {
        "drupal/core": {
          "We already have this patch for JS \"async\" and \"defer\" attributes": "https://git.drupalcode.org/project/refreshless/-/raw/9bd0325f415e95bfaa7adc29daaf26cc32bed673/modules/refreshless_turbo/patches/drupal/core/1587536-js-aggregation-async-defer-attributes.patch"
        }
      }
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        "[project-root]/.gitattributes": {
          "append": "drupal_scaffold/gitattributes_append"
        },
        "[web-root]/.ht.router.php": false,
        "[web-root]/.htaccess": {
          "prepend": "drupal_scaffold/htaccess_prepend",
          "append": "drupal_scaffold/htaccess_append"
        },
        "[web-root]/robots.txt": {
          "append": "drupal_scaffold/robots_append.txt"
        },
        "[web-root]/example.gitignore": false,
        "[web-root]/INSTALL.txt": false,
        "[web-root]/README.md": false,
        "[web-root]/web.config": false
      }
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
    }
  },
  "scripts": {
    "post-drupal-scaffold-cmd": [
      "patch --no-backup-if-mismatch -p1 <drupal_scaffold/editorconfig.patch"
    ]
  }
}
