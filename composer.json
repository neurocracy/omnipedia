  "require-dev": {
    "drupal/config_delete": "^2.0",
    "drupal/config_devel": "^1.2",
    "drupal/config_inspector": "^2.1",
    "drupal/core-dev": "^10.1@RC",
    "drupal/devel": "^5.0",
    "drupal/devel_entity_updates": "^4.0",
    "drupal/examples": "^4.0",
    "kint-php/kint": "^3.3",
    "phpspec/prophecy-phpunit": "^2",
    "symfony/var-dumper": "^5.0 || ^6.2"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "oomphinc/composer-installers-extender": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "drupal/core-disabled": {
        "JS aggregation should account for \"defer\" and \"async\" attributes [#1587536]: https://www.drupal.org/project/drupal/issues/1587536#comment-15106596": "patches/drupal/core/js-aggregation-defer-async-attributes-1587536-65.patch"
      },
      "drupal/honeypot": {
        "Missing primary key in table `honeypot_user` [#2943526]: https://www.drupal.org/project/honeypot/issues/2943526#comment-12712602": "patches/drupal/honeypot/honeypot-fix_missing_primary_key-8.x-2943526-6.patch"
      },
      "drupal/permissions_by_term": {
        "Permissions by Term complains about deactivated internal page cache when updating [#3254076]: https://www.drupal.org/project/permissions_by_term/issues/3254076#comment-14845306": "patches/drupal/permissions_by_term/3254076-internal-page-cache-warning-remove.patch",
        "Regression updating to 3.1.22 for Drupal 9.x using loadTemplate from twig [#3354478]: https://www.drupal.org/project/permissions_by_term/issues/3354478": "patches/drupal/permissions_by_term/3354478-twig-load-template-fatal-error.patch"
      },
      "drupal/readonlymode": {
        "Fix readonly notice message styling [#3008741]: https://www.drupal.org/project/readonlymode/issues/3008741": "patches/drupal/readonlymode/3008741-6-fix-readonly-notice-styling.patch",
        "Fatal error due to saved additional form IDs being strings and not arrays [#3115119]: https://www.drupal.org/project/readonlymode/issues/3115119": "patches/drupal/readonlymode/3115119-16-additional-forms-config-not-array.patch"
      },
      "drupal/session_limit": {
        "Displayed message should be configurable: https://www.drupal.org/project/session_limit/issues/2794553#comment-14364480": "patches/drupal/session_limit/session_limit-configurable-message-2794553-19.patch",
        "Services\\SessionLimit should type-hint ModuleHandlerInterface rather than a specific implementation [#3308506]: https://www.drupal.org/project/session_limit/issues/3308506": "patches/drupal/session_limit/3308506-typehint-module-handler-interface.patch"
      }
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "drupal/"
      },
      "file-mapping": {
        "[project-root]/.editorconfig": false,
        "[project-root]/.gitattributes": {
          "append": "drupal_scaffold/gitattributes_append.txt"
        },
        "[web-root]/.htaccess": {
          "prepend": "drupal_scaffold/htaccess_prepend.txt",
          "append": "drupal_scaffold/htaccess_append.txt"
        },
        "[web-root]/robots.txt": {
          "append": "drupal_scaffold/robots_append.txt"
        },
        "[web-root]/.ht.router.php": false,
        "[web-root]/example.gitignore": false,
        "[web-root]/INSTALL.txt": false,
        "[web-root]/README.md": false,
        "[web-root]/web.config": false
      }
    },
    "installer-paths": {
      "drupal/core": ["type:drupal-core"],
      "drupal/libraries/{$name}": ["type:drupal-library"],
      "drupal/modules/contrib/{$name}": ["type:drupal-module"],
      "drupal/profiles/contrib/{$name}": ["type:drupal-profile"],
      "drupal/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "drupal/modules/custom/{$name}": ["type:drupal-custom-module"],
      "drupal/themes/custom/{$name}": ["type:drupal-custom-theme"]
    }
  }
}
