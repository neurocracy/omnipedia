{
  "name": "omnipedia/drupal",
  "description": "Omnipedia Drupal site project",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://omnipedia.app",
  "authors": [
    {
      "name": "Ambient.Impact",
      "homepage": "https://ambientimpact.com/",
      "role": "Developer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    "wikimedia/composer-merge-plugin": {
      "type": "vcs",
      "url":  "https://github.com/mcaskill/composer-merge-plugin"
    }
  },
  "require": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6.5",
    "drupal/advagg": "^4.0",
    "drupal/config_split": "^1.6",
    "drupal/core-composer-scaffold": "^8.8",
    "drupal/core-recommended": "^8.8",
    "drupal/csp": "^1.13",
    "drupal/diff": "^1.0",
    "drupal/gin": "^3.0@alpha",
    "drupal/gin_toolbar": "^1.0@beta",
    "drupal/login_destination": "^1.0",
    "drupal/metatag": "^1.11",
    "drupal/passwordless": "^1.1",
    "drupal/path_alias_xt": "1.x-dev",
    "drupal/pathauto": "^1.6",
    "drupal/permissions_by_term": "^2.25",
    "drupal/remove_http_headers": "^1.0",
    "drupal/seckit": "^2.0",
    "drush/drush": "^10",
    "oomphinc/composer-installers-extender": "^2.0",
    "wikimedia/composer-merge-plugin": "dev-feature/composer-v2 as 1.5.0"
  },
  "require-dev": {
    "drupal/config_delete": "^1.7",
    "drupal/config_devel": "^1.2",
    "drupal/config_inspector": "^1.0.0-beta2",
    "drupal/devel": "^4.0",
    "drupal/devel_entity_updates": "^3.0",
    "drupal/examples": "^3.0",
    "drupal/migrate_plus": "^5.1",
    "drupal/migrate_tools": "^5.0",
    "drupal/migrate_upgrade": "^3.0.0",
    "kint-php/kint": "^3.3",
    "symfony/var-dumper": "^4.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {},
    "drupal-scaffold": {
      "locations": {
        "web-root": "drupal/"
      }
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "drupal/core": ["type:drupal-core"],
      "drupal/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "drupal/modules/contrib/{$name}": ["type:drupal-module"],
      "drupal/profiles/contrib/{$name}": ["type:drupal-profile"],
      "drupal/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "drupal/modules/custom/{$name}": ["type:drupal-custom-module"],
      "drupal/themes/custom/{$name}": ["type:drupal-custom-theme"]
    },
    "merge-plugin": {
      "include": [
        "drupal/modules/ambientimpact/ambientimpact_core/composer.json",
        "drupal/modules/ambientimpact/ambientimpact_block/composer.json",
        "drupal/modules/ambientimpact/ambientimpact_icon/composer.json",
        "drupal/modules/ambientimpact/ambientimpact_markdown/composer.json",
        "drupal/modules/ambientimpact/ambientimpact_media/composer.json",
        "drupal/modules/ambientimpact/ambientimpact_paragraphs/composer.json",
        "drupal/modules/ambientimpact/ambientimpact_ux/composer.json",
        "drupal/modules/omnipedia/omnipedia_core/composer.json",
        "drupal/modules/omnipedia/omnipedia_attached_data/composer.json",
        "drupal/modules/omnipedia/omnipedia_attached_data/omnipedia_attached_data_migrate/composer.json",
        "drupal/modules/omnipedia/omnipedia_block/composer.json",
        "drupal/modules/omnipedia/omnipedia_content/composer.json",
        "drupal/modules/omnipedia/omnipedia_content/omnipedia_content_legacy/composer.json",
        "drupal/modules/omnipedia/omnipedia_search/composer.json"
      ],
      "replace": false,
      "ignore-duplicates": true,
      "merge-extra": true,
      "merge-extra-deep": true
    }
  }
}
