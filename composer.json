    },
    "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "php": "^7.3|^8",
    "composer/installers": "^2.0",
    "cweagans/composer-patches": "^1.7",
    "drupal/advagg": "^4.0",
    "drupal/ambientimpact_core": "^4.0@dev",
    "drupal/ambientimpact_icon": "^4.0@dev",
    "drupal/ambientimpact_markdown": "^4.0@dev",
    "drupal/ambientimpact_media": "^4.0@dev",
    "drupal/ambientimpact_ux": "^4.0@dev",
    "drupal/autoban": "^1.7",
    "drupal/config_split": "^1.6",
    "drupal/core-composer-scaffold": "^9.3",
    "drupal/core-recommended": "^9.3",
    "drupal/csp": "^1.13",
    "drupal/dblog_persistent": "^2.0",
    "drupal/development_environment": "^2.0",
    "drupal/diff": "^1.0",
    "drupal/disable_link_rel": "^1.0",
    "drupal/eu_cookie_compliance": "^1.14",
    "drupal/fancy_file_delete": "^2.0",
    "drupal/flood_control": "^2.1",
    "drupal/ga_login": "^1.0@alpha",
    "drupal/gin": "^3.0@beta",
    "drupal/gin_toolbar": "^1.0@beta",
    "drupal/honeypot": "^2.0",
    "drupal/key": "^1.14",
    "drupal/login_destination": "^2.0",
    "drupal/login_security": "^2.0",
    "drupal/media_entity_file_replace": "^1.0",
    "drupal/metatag": "^1.11",
    "drupal/omnipedia_access": "^2.0@dev",
    "drupal/omnipedia_attached_data": "^2.0@dev",
    "drupal/omnipedia_block": "^2.0@dev",
    "drupal/omnipedia_commerce": "^2.0@dev",
    "drupal/omnipedia_content": "^2.0@dev",
    "drupal/omnipedia_content_changes": "^2.0@dev",
    "drupal/omnipedia_core": "^2.0@dev",
    "drupal/omnipedia_logger": "^2.0@dev",
    "drupal/omnipedia_media": "^2.0@dev",
    "drupal/omnipedia_search": "^2.0@dev",
    "drupal/omnipedia_site": "^2.0@dev",
    "drupal/omnipedia_user": "^2.0@dev",
    "drupal/password_policy": "^3.0",
    "drupal/pathauto": "^1.6",
    "drupal/redirect": "^1.6",
    "drupal/remove_http_headers": "^1.0",
    "drupal/rules": "^3.0@alpha",
    "drupal/seckit": "^2.0",
    "drupal/security_review": "^1.0@alpha",
    "drupal/session_limit": "^2.0@beta",
    "drupal/sodium": "^2.1",
    "drupal/subpathauto": "^1.2",
    "drupal/swiftmailer": "^2.0",
    "drupal/tfa": "^1.0@alpha",
    "drupal/token_filter": "^1.2",
    "drupal/views_bulk_edit": "^2.5",
    "drupal/views_bulk_operations": "^4.1",
    "drupal/webp": "^1.0",
    "drush/drush": "^11.0",
    "oomphinc/composer-installers-extender": "^2.0"
  },
  "require-dev": {
    "drupal/config_delete": "^1.7",
    "drupal/config_devel": "^1.2",
    "drupal/config_inspector": "^1.0.0-beta2",
    "drupal/core-dev": "^9.3",
    "drupal/devel": "^4.0",
    "drupal/devel_entity_updates": "^3.0",
    "drupal/examples": "^3.0",
    "kint-php/kint": "^3.3",
    "symfony/var-dumper": "^5.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "oomphinc/composer-installers-extender": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
      "consolidation/site-process": {
        "Windows: ProcessBase.php line 172: Unable to decode output into JSON: Syntax error: https://github.com/consolidation/site-process/issues/56": "patches/consolidation/site-process/windows-unable-to-decode-json.patch"
      },
      "drupal/development_environment": {
        "Duplicate key in menu links YAML file [#3218169]: https://www.drupal.org/project/development_environment/issues/3218169": "patches/drupal/development_environment/duplicate-key-3218169-2.patch"
      },
      "drupal/eu_cookie_compliance": {
        "Changes eu_cookie_compliance.js required for subsequent patch to succeed: https://www.drupal.org/project/eu_cookie_compliance/issues/3254209#comment-14355307": "patches/drupal/eu_cookie_compliance/merge-request-48-required-for-merge-request-49.patch",
        "Withdraw button not visible: https://www.drupal.org/project/eu_cookie_compliance/issues/3257088#mr49-note75081": "patches/drupal/eu_cookie_compliance/merge-request-49-withdraw-button-not-visible.patch"
      },
      "drupal/rules": {
        "Support upcasting entity IDs to full entity contexts: https://www.drupal.org/project/rules/issues/2800749#comment-14332836": "patches/drupal/rules/2800749-97.upcast.patch"
      },
      "drupal/security_review": {
        "Refactor trusted hosts check: https://www.drupal.org/project/security_review/issues/3008957": "patches/drupal/security_review/merge-request-1-refactor-trusted-hosts-check.patch",
        "Checks\\PrivateFiles: mistakes adjacent \"drupal_private_files\" as being under \"drupal\": https://www.drupal.org/project/security_review/issues/3259051": "patches/drupal/security_review/merge-request-2-private-files-check-more-robust.patch"
      },
      "drupal/session_limit": {
        "Displayed message should be configurable: https://www.drupal.org/project/session_limit/issues/2794553#comment-14364480": "patches/drupal/session_limit/session_limit-configurable-message-2794553-19.patch"
      }
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "drupal/"
      },
      "file-mapping": {
        "[web-root]/.htaccess": {
          "prepend": "drupal_scaffold/htaccess_prepend.txt"
        },
        "[web-root]/robots.txt": {
          "append": "drupal_scaffold/robots_append.txt"
        }
      }
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "drupal/core": ["type:drupal-core"],
      "drupal/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "drupal/modules/contrib/{$name}": ["type:drupal-module"],
      "drupal/profiles/contrib/{$name}": ["type:drupal-profile"],
      "drupal/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "drupal/modules/custom/{$name}": ["type:drupal-custom-module"],
      "drupal/themes/custom/{$name}": ["type:drupal-custom-theme"]
    }
  }
}
